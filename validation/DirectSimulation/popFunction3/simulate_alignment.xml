                <taxon spec="Taxon" id="t59"/>
                <taxon spec="Taxon" id="t60"/>
            </taxa>

            t1=0, t2=0, t3=0, t4=0, t5=0,
            t6=0, t7=0, t8=0, t9=0, t10=0,
